{
  "name": "Wslify",
  "version": "1.0",
  "description": "Modify file:// urls to point to wsl remote path.",
  "permissions": ["tabs", "activeTab", "http://*/", "https://*/", "file:///*", "file://*", "file://*/*", "declarativeContent", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/wslify_ico16.png",
      "32": "images/wslify_ico32.png",
      "48": "images/wslify_ico48.png",
      "128": "images/wslify_ico128.png"
    }
  },
  "manifest_version": 2
}